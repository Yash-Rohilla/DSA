class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int i,j,x=nums.size();
        for(i=0;i<x;i++)
        {
            for(j=1;j<x;j++)
            {
                if(i!=j)
                {
                if(nums[i]+nums[j]==target)
                {
                    return{i,j};
                }
                }
            }
        }
        return {0};
    }
};
